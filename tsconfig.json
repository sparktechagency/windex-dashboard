{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/admin/packages/components/TokenPackageModal.jsx",
    "src/app/admin/packages/components/packageTable.jsx",
    "src/app/admin/packages/components/TokenPackageEditModal.jsx",
    "src/app/admin/analytics/_components/UserOverAnalytics.jsx",
    "src/app/admin/refund-request/_components/RefundTable.js",
    "src/app/(Auth)/login/_components/SubAdminInvitationModal.jsx",
    "src/app/(Auth)/login/_components/SubAdminSchema.j/SubadminZod.js"
  ],
  "exclude": ["node_modules"]
}
